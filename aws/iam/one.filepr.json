{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*",
                "s3-object-lambda:Get*",
                "s3-object-lambda:List*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action" : "*",
            "Resource": [
                "arn:aws:s3:::iam-ex",
                "arn:aws:s3:::iam-ex/*"
                ]
        }
    ]
}
